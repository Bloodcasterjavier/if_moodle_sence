<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/if_sence_login/db" VERSION="20200906" COMMENT="XMLDB file for Moodle blocks/if_sence_login"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_if_sence_login" COMMENT="Guarda las sesiones exitosas. Campos son id,runalumno,idsesionalumno,idsesionsence,fechahora">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="runalumno" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="run del alumno"/>
        <FIELD NAME="idsesionalumno" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false" COMMENT="Id sesion en moodle"/>
        <FIELD NAME="idsesionsence" TYPE="char" LENGTH="150" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="codigocurso" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" COMMENT="codigo sence del curso"/>
        <FIELD NAME="fechahora" TYPE="char" LENGTH="19" NOTNULL="false" SEQUENCE="false" COMMENT="fechahora del login"/>
        <FIELD NAME="zonahoraria" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" COMMENT="zona horario del login"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>